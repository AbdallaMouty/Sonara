{
  "language": "Sonara",
  "intro": "A lingua construs, disinita por bela e simpa, con a vosa cantus e aurita, animita de Latin, Japanese, Sanskrit, Arabic, Hebrew, e utros orivas.",
  "description": "A constructed language, designed for beauty and simplicity, with a melodic, flowy sound inspired by Latin, Japanese, Sanskrit, Arabic, Hebrew, and other sources.",
  "examples": [
    {
      "example": "La toma fidus librar la cora de ira malus.",
      "translation": "The faithful friend frees the heart from evil anger."
    },
    {
      "example": "La sola viam por farar laboro mirus is amar la laboro vua.",
      "translation": "The only way to do wonderful work is to love your work."
    },
    {
      "example": "En la media de la dura espar la oportuna.",
      "translation": "In the middle of difficulty lies the opportunity."
    },
    {
      "example": "La cora gardar suas causas, que la ratio ignorar.",
      "translation": "The heart keeps its reasons, which reason ignores."
    },
    {
      "example": "La vera va librar vus.",
      "translation": "The truth will free you."
    }
  ],
  "phonology": [
    {
      "aspect": "Vowels",
      "description": "/a, e, i, o, u/ (pronounced as in Spanish: ah, eh, ee, oh, oo). Examples: luna /lu.na/, sera /se.ra/."
    },
    {
      "aspect": "Consonants",
      "description": "Soft sounds: /p, b, t, d, g, f, v, s, m, n, l, r, j/. c = /k/ (e.g., canta /kan.ta/). No harsh sounds or nasals."
    },
    {
      "aspect": "Stress",
      "description": "Penultimate syllable unless marked with diacritics (á, é, í, ó, ú, e.g., sófte /sof.te/, softly)."
    },
    {
      "aspect": "Feel",
      "description": "Smoother than English/Italian, as melodic as French/Portuguese, minimal diphthongs."
    }
  ],
  "grammar": [
    {
      "rule": "Word Order",
      "description": "Subject-Verb-Object (SVO, e.g., Mi amar la luna, I love the moon)."
    },
    {
      "rule": "Nouns",
      "description": "End in -a or -o for euphony. Person-related: -o (male), -a (female, e.g., tomo/toma, friend male/female). Plural: add -s (e.g., lunas, moons)."
    },
    {
      "rule": "Articles",
      "description": "Definite: la (singular/plural, e.g., la luna, la lunas). Indefinite: a (singular), as (plural, e.g., a luna, as lunas)."
    },
    {
      "rule": "Pronouns",
      "description": "mi (I), vu (you, singular), vi (he/she/it), na (we), vus (you, plural), vis (they)."
    },
    {
      "rule": "Possessives",
      "description": "mia (my), vua (your, for vu/vus), sua (his/her/their), nua (our)."
    },
    {
      "rule": "Verbs",
      "description": "Infinitive: -ar (e.g., amar, to love). Present: infinitive + pronoun (e.g., mi amar, I love). Past: -i (e.g., amari, loved). Future: va + infinitive (e.g., va amar, will love). Progressive: -an (e.g., amaran, loving)."
    },
    {
      "rule": "Adjectives",
      "description": "Invariable, follow nouns (e.g., sama lumus, glowing sky)."
    },
    {
      "rule": "Adverbs",
      "description": "Add -ita to adjectives (e.g., lumus → lumita, glowingly)."
    },
    {
      "rule": "Copula",
      "description": "is (to be, present, e.g., La hana is salus, The flower is peaceful)."
    }
  ],
